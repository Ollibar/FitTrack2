<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tV_list_ziel_name_Kraft"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="57dp"
        android:layout_marginTop="54dp"
        android:layout_weight="1"
        android:text="@string/tV_zielKraft"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tV_list_ziel_Gewicht"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tV_list_ziel_name_Kraft"
        android:layout_marginTop="88dp"
        android:layout_weight="1"
        android:text="TextView" />

    <TextView
        android:id="@+id/tV_list_ziel_Dauer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tV_list_ziel_name_Kraft"
        android:layout_centerVertical="true"
        android:layout_weight="1"
        android:text="TextView" />

    <TextView
        android:id="@+id/tV_zielPositionEins"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tV_list_ziel_Gewicht"
        android:layout_marginEnd="83dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tV_zielPositionZwei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tV_zielPositionEins"
        android:layout_marginTop="123dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tV_zielTrainPosDrei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tV_zielPositionEins"
        android:layout_marginTop="156dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tV_zielKonditionTrain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/tV_list_ziel_name_Kraft"
        android:layout_marginTop="217dp"
        android:text="@string/tVZielTrainKondi"
        android:textColor="@android:color/holo_blue_dark"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tV_zielTrainGesw"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tV_zielPositionEins"
        android:layout_centerVertical="true"
        android:text="TextView" />

    <TextView
        android:id="@+id/tV_zielBenutzerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/tV_list_ziel_name_Kraft"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="-54dp"
        android:layout_marginStart="16dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tV_zielStationName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tV_zielBenutzerName"
        android:layout_below="@+id/tV_list_ziel_name_Kraft"
        android:text="TextView" />
</RelativeLayout>